<%= name %>Routing = ($stateProvider) ->
  $stateProvider.state '<%= name %>',
    url: '/<%= name %>'
    templateUrl: "app/<%= name %>/<%= name %>.html"
    controller: '<%= name %>Controller'
    controllerAs: '<%= name %>'

  return

<%= name %>Routing.$inject = ['$stateProvider']

angular.module('<%= name %>').config(<%= name %>Routing)

